{
  "name": "laoergege-blog",
  "version": "1.0.1",
  "description": "Laoergege's Learning Track",
  "scripts": {
    "docs:dev": "turbo run dev",
    "dev": "pnpm run vuepress -- dev docs",
    "docs:build": "turbo run build",
    "build": "pnpm run vuepress -- build docs",
    "vuepress": "node index.mjs",
    "release": "git push && git tag page -f && git push origin page -f",
    "package:publish": "turbo run build --filter='./packages/*' && pnpm -r --filter ./packages/* exec publish",
    "open": "open-cli 'https://daisyui.com/' && open-cli 'https://tailwindcss.com/docs/installation'",
    "demo": "pnpm exec node demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laoergege/laoergege-blog.git"
  },
  "keywords": [
    "laoergege",
    "blog",
    "javascript",
    "vue",
    "react",
    "webpack",
    "pwa",
    "node"
  ],
  "author": "laoergege",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/laoergege/laoergege-blog/issues"
  },
  "homepage": "https://github.com/laoergege/laoergege-blog#readme",
  "dependencies": {
    "@laoergege/vuepress-plugin-blog-utils": "workspace:^1.0.0",
    "@laoergege/vuepress-plugin-vssue-next-compat": "workspace:^1.0.2",
    "@tailwindcss/typography": "0.5.2",
    "@vssue/api-github-v3": "^1.4.7",
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "daisyui": "2.8.0",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@laoergege/scripts": "workspace:^1.0.0",
    "@types/node": "*",
    "@vuepress/bundler-webpack": "^2.0.0-beta.46",
    "@vuepress/cli": "^2.0.0-beta.46",
    "@vuepress/client": "^2.0.0-beta.46",
    "@vuepress/core": "^2.0.0-beta.46",
    "@vuepress/plugin-active-header-links": "^2.0.0-beta.46",
    "@vuepress/plugin-back-to-top": "^2.0.0-beta.46",
    "@vuepress/plugin-external-link-icon": "^2.0.0-beta.46",
    "@vuepress/plugin-git": "^2.0.0-beta.46",
    "@vuepress/plugin-medium-zoom": "^2.0.0-beta.46",
