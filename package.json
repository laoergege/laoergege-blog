    "autoprefixer": "^10.4.7",
    "chokidar": "^3.5.3",
    "cssnano": "^5.1.10",
    "dayjs": "^1.11.2",
    "isomorphic-git": "^1.17.3",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.0",
    "sass": "^1.52.1",
    "sass-loader": "^12.6.0",
    "stylus": "^0.58.1",
    "stylus-loader": "^7.0.0",
    "tailwindcss": "^3.0.24",
    "turbo": "^1.2.15",
